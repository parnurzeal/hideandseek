# sublime
alias subln="/Applications/Sublime\ Text\ 2.app/Contents/SharedSupport/bin/subl -n"

# excuberant ctags
alias ctags=/usr/local/Cellar/ctags/5.8/bin/ctags

# screen sharing in mac os x
alias vnc="open /System/Library/CoreServices/Screen\ Sharing.app"

# ls
alias ls='ls -G'
alias ll='ls -hl'


